{	"extends": "@tsconfig/svelte/tsconfig.json",
	"include": ["src/**/*", "src/node_modules"],	"exclude": ["node_modules/*", "__sapper__/*", "public/*"],
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "Bundler",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "strictNullChecks":false,
        "rootDirs": [
            ".",
            "./.svelte-kit/types",
        ]
    }
}